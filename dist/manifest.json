{
  "manifest_version": 3,
  "name": "토게 인포 검색 변경기",
  "version": "1.0.0",
  "description": "Google 검색 결과를 토게 인포로 변경하고, 변경 횟수를 팝업에서 표시하는 크롬 확장",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": "icons/icon48.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-Bi1dhHjH.js"
      ],
      "matches": [
        "https://www.google.com/search*"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://www.google.com/*"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "resources": [
        "assets/content.ts-Bi1dhHjH.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
